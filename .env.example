# Multi-environment LiveKit configuration
# Use PRD_ prefix for production and DEV_ prefix for development

# Production credentials
PRD_LIVEKIT_API_KEY=<your_prd_api_key>
PRD_LIVEKIT_API_SECRET=<your_prd_api_secret>
PRD_LIVEKIT_URL=wss://<prd-project-subdomain>.livekit.cloud
PRD_AGENT_NAME=<your_prd_agent_name>
PRD_OUTBOUND_TRUNK_LIST=[{"id": "ST_xxx", "name": "[PRD] Trunk Name"}]

# Development credentials
DEV_LIVEKIT_API_KEY=<your_dev_api_key>
DEV_LIVEKIT_API_SECRET=<your_dev_api_secret>
DEV_LIVEKIT_URL=wss://<dev-project-subdomain>.livekit.cloud
DEV_AGENT_NAME=<your_dev_agent_name>
DEV_OUTBOUND_TRUNK_LIST=[{"id": "ST_xxx", "name": "[DEV] Trunk Name"}]

# Internally used environment variables
NEXT_PUBLIC_APP_CONFIG_ENDPOINT=
SANDBOX_ID=
