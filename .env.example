# Multi-environment LiveKit configuration
# Use PRD_ prefix for production and DEV_ prefix for development

# Production credentials
PRD_LIVEKIT_API_KEY=<your_prd_api_key>
PRD_LIVEKIT_API_SECRET=<your_prd_api_secret>
PRD_LIVEKIT_URL=wss://<prd-project-subdomain>.livekit.cloud
PRD_AGENT_NAME=<your_prd_agent_name>
PRD_OUTBOUND_TRUNK_LIST=[{"id": "ST_xxx", "name": "[PRD] Trunk Name"}]

# Development credentials
DEV_LIVEKIT_API_KEY=<your_dev_api_key>
DEV_LIVEKIT_API_SECRET=<your_dev_api_secret>
DEV_LIVEKIT_URL=wss://<dev-project-subdomain>.livekit.cloud
DEV_AGENT_NAME=<your_dev_agent_name>
DEV_OUTBOUND_TRUNK_LIST=[{"id": "ST_xxx", "name": "[DEV] Trunk Name"}]

# Development BP credentials
DEV_BP_LIVEKIT_API_KEY=<your_dev_bp_api_key>
DEV_BP_LIVEKIT_API_SECRET=<your_dev_bp_api_secret>
DEV_BP_LIVEKIT_URL=wss://<dev-bp-project-subdomain>.livekit.cloud
DEV_BP_AGENT_NAME=<your_dev_bp_agent_name>
DEV_BP_OUTBOUND_TRUNK_LIST=[{"id": "ST_xxx", "name": "[DEV_BP] Trunk Name"}]

# Production BP credentials
PRD_BP_LIVEKIT_API_KEY=<your_prd_bp_api_key>
PRD_BP_LIVEKIT_API_SECRET=<your_prd_bp_api_secret>
PRD_BP_LIVEKIT_URL=wss://<prd-bp-project-subdomain>.livekit.cloud
PRD_BP_AGENT_NAME=<your_prd_bp_agent_name>
PRD_BP_OUTBOUND_TRUNK_LIST=[{"id": "ST_xxx", "name": "[PRD_BP] Trunk Name"}]

# Local credentials
LOCAL_LIVEKIT_API_KEY=<your_local_api_key>
LOCAL_LIVEKIT_API_SECRET=<your_local_api_secret>
LOCAL_LIVEKIT_URL=ws://localhost:7880
LOCAL_AGENT_NAME=<your_local_agent_name>
LOCAL_OUTBOUND_TRUNK_LIST=[{"id": "ST_xxx", "name": "[LOCAL] Trunk Name"}]

# Internally used environment variables
NEXT_PUBLIC_APP_CONFIG_ENDPOINT=
SANDBOX_ID=
